@article{Ralston1978,
abstract = {The high-energy process {\$}p+p\backslashrightarrow \backslashmu{\^{}}+ + \backslashmu{\^{}}- + X{\$} for dimuons with large invariant mass is studied in the case that the protons are polarized. In general, the cross section {\$}d\backslashsigma/\backslashdQ{\^{}}2 dt d\backslashOmega{\$} is determined by nine structure functions, and several interesting correlations between the polarization direction and the observed cross section are possible. In the Drell-Yan parton model for the process, the structure of the cross section is much simpler: there are six linear relations among the structure functions. An experimental test of these relations would provide a stringent test of the Drell-Yan model.},
