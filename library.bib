@article{Baldes1978,
abstract = {Ever since Luria's essay on ancient atomism1 began to be read with more than passing interest, a major issue in Democritean studies has been the question whether or not Democritus' atom was intended to be mathematically divisible. The array of com- petent scholars ranging themselves on either side of the question is impressive.2 Those who hold that it was mathematically indivisible are led to this opinion primarily by the mathematical nature of Zeno's arguments and by Aristotle's reformulation of those arguments in explaining the foundations of Democritus' atomic theory. This essay attempts to defend the claim that Democritus' atom was as mathematically divisible, to integrate Democritus' view of divisibility into the distinction he made between statements about perceived objects and those about atoms and void, and, thus, to suggest a new light in which one may view the nature of Democritus' response to Zeno's arguments},
author = {Baldes, Richard W.},
file = {:home/jollyhrothgar/Dropbox/physics/reference{\_}papers/40913402.pdf:pdf},
journal = {Apeiron: A Journal for Ancient Philosophy and Science},
number = {1},
pages = {1--12},
title = {{'Divisibility' and 'Division' in Democritus}},
url = {http://www.jstor.org/stable/40913402},
volume = {12},
year = {1978}
