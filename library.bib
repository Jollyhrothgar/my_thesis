@techreport{Kim2014,
abstract = {This analysis note describes the studies performed to confirm the information currently contained in the spin database for the 2013 running period. The additional information was taken from the local polarimeter analysis, the most recent RHIC polarimetry information, the rhicscaler database, PHENIX and CAD elogs as well as independent crossing shift studies using muon candidates. A run list for runs which passed out QA procedure is included in this note and can be used for spin analysis. However, as the spin database information might change, especially with updated final numbers from the polarimeter group it should not be viewed as a guarantee to trust all the information in the spin database blindly.. We generally recommend every spin analyzer to at least confirm the correct crossing shift when using the spin database. At present 1008 runs of Run13 are available on the Analysis Taxi. Currently 48 of these runs do not contain spin information in the database due to lacking polarimetry information at this stage. Furthermore 4 runs are flagged by suspicious crossing shift information and about 38 runs have some concerns with the scalers but are otherwise useable. The previous runs that were havng incorrectly assigned spin patterns are fixed now.},
author = {Beaumier, Mike and Gal, Ciprian and Kim, Minjung and Park, Sanghwa and Seidl, R and Yoon, Inseok},
doi = {AN1125},
file = {:home/jollyhrothgar/Dropbox/physics/reference{\_}papers/an1125{\_}spin{\_}db{\_}qa.pdf:pdf},
keywords = {PHENIX,Spin Physics},
mendeley-tags = {PHENIX,Spin Physics},
pages = {1--29},
title = {{Run 13 Spin Database Quality Assurance}},
year = {2014}
}
